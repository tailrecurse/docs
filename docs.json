{
  "$schema": "https://mintlify.com/schema.json",
  "theme": "mint",
  "name": "Jupiter Bots Documentation",
  "colors": {
    "primary": "#0D9373",
    "dark": "#0D9373"
  },
  "favicon": "/favicon.ico",
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Comet Predict Bot",
        "icon": "meteor",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "prediction-bot/getting-started/introduction",
              "prediction-bot/getting-started/links",
              "prediction-bot/getting-started/faqs"
            ]
          },
          {
            "group": "Prediction Bot",
            "pages": [
              "prediction-bot/notifications",
              "prediction-bot/wallet",
              "prediction-bot/commands"
            ]
          }
        ]
      },
      {
        "tab": "Comet Trading Bot",
        "icon": "comet",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "trading-bot/getting-started/introduction",
              "trading-bot/getting-started/links",
              "trading-bot/getting-started/faqs"
            ]
          },
          {
            "group": "Trading Bot",
            "pages": ["trading-bot/wallet", "trading-bot/commands"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Launch Comet Predict Bot",
          "icon": "meteor",
          "href": "https://t.me/jup_pred_bot"
        },
        {
          "anchor": "Launch Comet Trading Bot",
          "icon": "comet",
          "href": "https://t.me/jup_comet_bot"
        },
        {
          "anchor": "Community",
          "icon": "discord",
          "href": "https://discord.gg/jup"
        }
      ]
    }
  },
  "logo": "/logo/logo.png",
  "css": "/custom.css",
  "footer": {
    "socials": {
      "x": "https://x.com/jup_ag",
      "discord": "https://discord.gg/jup"
    }
  }
}
